<Window x:Class="MyClock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyClock"
        
        mc:Ignorable="d"
        Title="MainWindow" Height="469" Width="1112">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="2*" />
        </Grid.ColumnDefinitions>
        <local:AnologClockControl Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <local:DigitalClockControl Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" />
    </Grid>
</Window>
